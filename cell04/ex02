i = input("Give me the first number: ")
i1 = int(i)
o = input("Give me the second number: ")
o1 = int(o)

e1 = i1 + o1
e2 = i1 - o1
e3 = i1 * o1
e4 = i1 / o1
print( i1 , "+" , o1   , "=" , e1 )
print( i1 , "-" ,  o1  , "=" , e2 )
print( i1 , "x" ,  o1  , "=" ,e3  )
print( i1 , "/" ,  o1  , "=" , e4 )
